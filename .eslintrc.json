{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "extends": [
        "plugin:lvksh/recommended"
    ],
    "ignorePatterns": [
        "!**/*"
    ],
    "plugins": [
        "lvksh"
    ],
    "env": {
        "node": true
    },
    "rules": {
        "sonarjs/cognitive-complexity": "off"
    }
}